{
    "title": "私人笔记",
    "author": "cofire",
    "description": "select * from learn",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "printLog": false,
    "styles": {
        "website": "./static/style/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "prism",
        "-sharing",
        "sharing-plus",
        "favicon",
        "code",
        "copy-code-button",
        "-back-to-top-button",
        "-toggle-chapters",
        "chapter-fold",
        "favicon-absolute",
        "pageview-count",
        "anchor-navigation-ex"
    ],
    "pluginsConfig": {
        "anchor-navigation-ex": {
            "showLevel": true,
            "associatedWithSummary": false,
            "printLog": false
        },
        "sharing": {
            "weibo": true,
            "qq": true
        },
        "favicon-absolute": {
            "favicon": "https://cofire.github.io/book/static/icon/favicon.ico",
            "bookmark": "https://cofire.github.io/book/static/icon/favicon.ico",
            "appleTouchIconPrecomposed152": "https://cofire.github.io/book/static/icon/favicon.ico"
        },
        "theme-default": {
            "showLevel": true
        }
    }
}