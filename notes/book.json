{
    "title": "cofire's Glory",
    "author": "cofire",
    "description": "select * from learn",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "./static/style/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "-sharing",
        "prism",
        "-highlight",
        "sharing-plus",
        "favicon",
        "code",
        "copy-code-button",
        "back-to-top-button",
        "toggle-chapters",
        "multipart"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/cofire"
        },
        "anchor-navigation-ex": {
            "showLevel": false
        },
        "sharing": {
            "facebook": true,
            "twitter": true,
            "weibo": true,
            "qq": true,
            "all": [
                "douban",
                "google",
                "instapaper",
                "linkedin",
                "twitter",
                "messenger",
                "qzone",
                "viber",
                "whatsapp"
            ]
        },
        "favicon": {
            "shortcut": "./static/icon/favicon.ico",
            "bookmark": "./static/icon/favicon.ico",
            "appleTouch": "./source/images/apple-touch-icon.jpg",
            "appleTouchMore": {
                "120x120": "./source/images/apple-touch-icon.jpg",
                "180x180": "./source/images/apple-touch-icon.jpg"
            }
        },
        "theme-default": {
            "showLevel": true
        }
    }
}